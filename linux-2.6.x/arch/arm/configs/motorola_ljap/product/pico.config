#
# arch/arm/configs/motorola_ljap/product/pico.config
#
# Pico-specific Linux kernel configuration options. This file is evaluated
# by Kconfig following the evaluation of these files:
#      arch/arm/configs/motorola_ljap_defconfig
#      arch/arm/configs/motorola_ljap/product-family/scma11.config
#
# Copyright 2006 Motorola, Inc.
#

CONFIG_MACH_PICO=y
CONFIG_MOT_FEAT_FLIP=y
# CONFIG_MOT_FEAT_PM_DESENSE is not set

#
# GPIO API Options
#
CONFIG_MOT_FEAT_GPIO_API=y
CONFIG_MOT_FEAT_GPIO_API_MC13783=y
CONFIG_MOT_FEAT_GPIO_API_BTPOWER=y
CONFIG_MOT_FEAT_GPIO_API_CSI=y
CONFIG_MOT_FEAT_GPIO_API_DAI=y
# CONFIG_MOT_FEAT_GPIO_API_ETHERNET is not set
# CONFIG_MOT_FEAT_GPIO_API_ETM is not set
CONFIG_MOT_FEAT_GPIO_API_LCD=y
CONFIG_MOT_FEAT_GPIO_API_LIGHTING=y
# CONFIG_MOT_FEAT_GPIO_API_LIGHTING_LCD is not set
# CONFIG_MOT_FEAT_GPIO_API_LIGHTING_CAM_TORCH is not set
CONFIG_MOT_FEAT_GPIO_API_LIGHTING_CAM_FLASH=y
CONFIG_MOT_FEAT_GPIO_API_LIGHTING_EL=y
# CONFIG_MOT_FEAT_GPIO_API_SDHC is not set
# CONFIG_MOT_FEAT_GPIO_API_USBHS is not set
# CONFIG_MOT_FEAT_GPIO_API_WLAN is not set
